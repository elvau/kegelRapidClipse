<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<?xdevuiml version="1.0"?>
<?template BROWSER_WUXGA?>
<?import com.vaadin.flow.component.html.H1?>
<?import com.vaadin.flow.component.html.Label?>
<?import com.vaadin.flow.component.orderedlayout.HorizontalLayout?>
<?import com.vaadin.flow.component.orderedlayout.VerticalLayout?>
<?import com.vaadin.flow.component.textfield.NumberField?>
<VerticalLayout xmlns:x="http://xml.xdev-software.com/xdevuiml" x:name="TeamSpielErstellen" x:route="TeamSpielErstellen">
    <x:constraints zpos="0" width="100%" height="100%" />
    <H1 x:name="h1" text="Teamspiel erstellen">
        <x:constraints width="0px" alignment="CENTER" height="0px" flex-grow="0.0" />
    </H1>
    <HorizontalLayout x:name="horizontalLayout">
        <x:constraints width="-1px" alignment="CENTER" height="-1px" flex-grow="0.0" />
        <VerticalLayout x:name="verticalLayout">
            <x:constraints width="-1px" height="-1px" flex-grow="0.0" />
            <Label x:name="label" text="Wie viele Teams?">
                <x:constraints width="0px" height="0px" flex-grow="0.0" />
            </Label>
            <NumberField x:name="numberField">
                <x:constraints width="0px" height="0px" flex-grow="0.0" />
            </NumberField>
        </VerticalLayout>
    </HorizontalLayout>
</VerticalLayout>
